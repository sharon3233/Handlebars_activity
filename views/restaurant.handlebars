<div>
	<h2>Single Restaurant: {{restaurant.name}}</h2>
	<h3>  {{restaurant.name}} </h3>
	<img class="item-img" src="{{restaurant.image}}" />
	<a class="btn" href="/restaurants">See my Restaurant List</a>
</div>


<p>{{#each restaurant.Menus}}

<h5>{{this.title}}</h5>

<ul>
	{{this.MenuItems}}

	<li> {{this.name}}   : ${{this.price}} </li>
	</ul>
{{/each}}
</p>
<a href="/restaurants"></a>


